{
  "cli": {
    "version": ">= 16.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.11.1",
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "20.11.1",
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1"
      },
      "android": {
        "buildType": "apk",
        "resourceClass": "default"
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "20.11.1",
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
